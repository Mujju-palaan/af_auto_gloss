{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/navbar-footer/Navbar.jsx", "app/aaContainer/service/supportpage/supportpage.js", "app/(service)/support/page.tjs", "components/contact/contactformcomponents/home/Newsletter.jstter.js", "components/home/Content.jsx", "components/product/Productcardpage.jsx"],
  
  "exclude": ["node_modules"]
}
